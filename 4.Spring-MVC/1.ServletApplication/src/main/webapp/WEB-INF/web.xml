<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AdditionController</servlet-name>
  	<display-name>AdditionController</display-name>
  	<description></description>
  	<servlet-class>com.web.AdditionController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SubtractionController</servlet-name>
  	<display-name>SubtractionController</display-name>
  	<description></description>
  	<servlet-class>com.web.SubtractionController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MultiplicationController</servlet-name>
  	<display-name>MultiplicationController</display-name>
  	<description></description>
  	<servlet-class>com.web.MultiplicationController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AdditionController</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SubtractionController</servlet-name>
  	<url-pattern>/sub</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MultiplicationController</servlet-name>
  	<url-pattern>/mul</url-pattern>
  </servlet-mapping>
  
</web-app>
